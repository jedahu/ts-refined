{
    "compilerOptions": {
        "alwaysStrict": true,
        "newLine": "lf",
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "strictNullChecks": true,
        "declaration": true,
        "outDir": "./dist",
        "target": "es6"
    },
    "include": ["./src/*.ts"],
    "exclude": []
}
